{
  "name": "amira-letter-scoring-cdk",
  "version": "1.0.0",
  "description": "CDK infrastructure for Amira Letter Scoring GPU pipeline",
  "main": "dist/amira-letter-scoring-stack.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy -a 'node dist/bin/amira-letter-scoring.js'",
    "destroy": "cdk destroy -a 'node dist/bin/amira-letter-scoring.js'",
    "deploy-parallel": "cdk deploy -a 'node dist/bin/amira-lambda-parallel.js' AmiraLambdaParallelStack",
    "destroy-parallel": "cdk destroy -a 'node dist/bin/amira-lambda-parallel.js' AmiraLambdaParallelStack"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.212.0",
    "constructs": "10.4.2",
    "source-map-support": "^0.5.21"
  }
}
